<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ERPSYS.MVC"
                   namespace="ERPSYS.MVC.Models">
  <class name="Fornecedor">
    <cache usage="nonstrict-read-write"/>
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="NomeFantasia"/>
    <property name="NomeRazaoSocial" />
    <property name="InscricaoEstadual" />
    <property name="CNPJ" />
    <property name="Email" />
    <property name="TelefoneUm" />
    <property name="TelefoneDois" />
    <property name="Observacoes" />
    <property name="DataInclusao" />
    <property name="DataAlteracao" />
    <one-to-one name="UsuarioInclusao" class="Usuario" />
    <one-to-one name="UsuarioAlteracao" class="Usuario" />
  </class>
</hibernate-mapping>