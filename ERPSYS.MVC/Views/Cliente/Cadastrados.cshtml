<title>Lista de Clientes</title>
<div class="main-content">
    <div class="content-heading clearfix">
        <div class="heading-left">
            <h1 class="page-title">Cadastros</h1>
            <p class="page-subtitle">Lista de clientes</p>
        </div>
        <ul class="breadcrumb">
            <li><a href="#"><i class="fa fa-home"></i> Home</a></li>
            <li><a href="#">Cadastros</a></li>
            <li><a href="#">Lista de Clientes</a></li>
        </ul>
    </div>

    <div class="panel demo display">
        <div class="panel-heading">
            <h3 class="panel-title">Lista de Clientes</h3>
        </div>
        <div class="panel-body text-center">

            <div>
                <button type="button" class="btn btn-success btn-lg" onclick="window.location.href='/Cliente/Novo'">Adicionar</button>
                <button type="button" class="btn btn-info btn-lg" id="editar">Editar</button>
                <button type="button" class="btn btn-danger btn-lg" onclick="" id="inativar">Inativar</button>
                <button type="button" class="btn btn-success btn-lg" onclick="" id="ativar">Ativar</button>
            </div>

            <table id="MyEstoque" class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Nome</th>
                        <th>CPF</th>
                        <th>Pontos</th>
                        <th>Situação</th>
                        <th>Data cadastro</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/Template/TemplateADMIN/assets/scripts/templateAdminScripts.js"></script>
    <script src="~/js/Grid/gridClientes.js"></script>
}